{
  "manifest_version": 2,
  "name": "Onelec mail.yandex.ru расширение (OME)",
  "version": "1.0.9",
  "description": "Chrome-расширение для менеджеров onelec",
  "author": "t.me/im0rg",
  "permissions": [
    "clipboardRead",
    "cookies",
    "history",
    "management",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [ "*://mail.yandex.ru/*" ],
      "js": [ "content.js" ],
      "css": [ "style.css" ]
    }
  ]
}
